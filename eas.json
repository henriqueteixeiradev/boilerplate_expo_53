{
  "cli": {
    "version": ">= 4.1.2",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      },
      "ios": {
        "simulator": false,
        "autoIncrement": true
      },
      "env": {}
    },
    "rc": {
      "channel": "production",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      },
      "ios": {
        "simulator": false,
        "autoIncrement": true
      },
      "env": {}
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "ios": {
        "simulator": false,
        "autoIncrement": true
      },
      "env": {}
    }
  }
}
